        "type": "function",
        "z": "487a207a097581f8",
        "name": "function 1",
        "func": "let logs = context.get(\"logs\") || [];\n\nconst now = new Date();\nconst timeStr = now.toLocaleString();\n\nlogs.unshift({\n    time: timeStr,\n    event: msg.payload || \"TAKEN\"\n});\n\nlogs = logs.slice(0, 50);\ncontext.set(\"logs\", logs);\n\nmsg.payload = logs;\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
